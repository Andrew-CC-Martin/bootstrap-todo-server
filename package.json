{
  "name": "bootstrap-todo-server",
  "version": "1.0.0",
  "description": "backend for basic todo app",
  "engines": {
    "node": "12.16.3"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start:dev": "nodemon",
    "start": "node index.js",
    "lint": "eslint --ext .js .",
    "migrate": "sequelize db:migrate",
    "restoredb": "pg_restore -v -c -x -O -h localhost -U postgres -d database_development latest.dump",
    "prepush": "npm run lint && npm test"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepush"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Andrew-CC-Martin/bootstrap-todo-server.git"
  },
  "author": "Andrew Martin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Andrew-CC-Martin/bootstrap-todo-server/issues"
  },
  "homepage": "https://github.com/Andrew-CC-Martin/bootstrap-todo-server#readme",
  "dependencies": {
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "pg": "8.2.1",
    "pg-hstore": "2.3.3",
    "sequelize": "5.21.12",
    "sequelize-cli": "5.5.1"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.13.2",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "nodemon": "2.0.4",
    "typescript": "3.9.5"
  }
}
